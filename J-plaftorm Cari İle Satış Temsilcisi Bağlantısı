
SELECT  ARP.CODE AS [Cari Kodu],ARP.DESCRIPTION as [Cari Adı],
ARP.CREATEDON[AÇILIŞ TARİHİ],
SLS.DESCRIPTION as  [Satış Elemanı Adı]
FROM     dbo.U_009_ARPS ARP FULL JOIN
                  dbo.U_009_ARPSLSMNASGS ASG ON ARP.LOGICALREF = ASG.ARPREF FULL JOIN
                  dbo.U_009_SALESPERSONS SLS ON ASG.SALESMANREF = SLS.LOGICALREF
				  WHERE ARP.AUXCODE5 IN ('') 
GO
