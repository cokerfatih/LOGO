SELECT JOURNALNO, date_ FROM LG_215_01_EMFICHE WHERE DATE_ BETWEEN '2015-31-03 00:00:00.000' AND '2015-31-03 00:00:00.000'
SELECT JOURNALNO, date_ FROM LG_215_01_EMFICHE WHERE DATE_ BETWEEN '2015-12-01 00:00:00.000' AND '2015-12-31 00:00:00.000'

USE lgk2v3
UPDATE LG_215_01_EMFICHE  SET JOURNALNO='0' WHERE DATE_ BETWEEN '2015-12-01 00:00:00.000' AND '2015-12-31 00:00:00.000'
UPDATE LG_215_01_EMFLINE  SET GLOBLINENO='0' WHERE DATE_ BETWEEN '2015-12-01 00:00:00.000' AND '2015-12-31 00:00:00.000'
