WHERE ACCFICHEREF NOT IN(SELECT LOGICALREF FROM LG_005_01_EMFICHE)
SELECT * FROM LG_005_01_EMFICHE
WHERE LOGICALREF NOT IN(SELECT ACCFICHEREF  FROM LG_005_01_EMFLINE)


SELECT * FROM LG_005_01_EMFLINE WHERE ACCOUNTREF='2283'
